[color]
	branch      = auto
	diff        = auto
	grep        = auto
	interactive = auto
	status      = auto
	ui          = auto
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
	quotepath = false
[user]
	name = Koki Sato
	email = admin@koki-sato.com
	signingkey = admin@koki-sato.com
[github]
	user = koki-sato
[commit]
	gpgsign = false
[push]
	default = current
[rebase]
	autosquash = true
[include]
	path = ~/.gitconfig.local
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
